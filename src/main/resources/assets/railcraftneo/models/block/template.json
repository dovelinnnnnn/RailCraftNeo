{
  "parent": "", //(可选）从给定的命名空间ID中加载模型文件，作为本模型的父模型。父模型中的属性将继承到本模型中，但在本模型内声明的属性会覆盖父模型的属性。父模型不能为自身，也不能循环继承。
  "display":{//可选，若无继承属性默认为无变换）列出方块对应的物品模型于不同位置下的显示设置。
    "thirdperson_righthand": {
      "scale": [1,1,1],//（可选，默认为[1, 1, 1]）使模型相对于对应轴进行缩放，每个轴上的缩放都钳制在-4到4之间。
      "rotation": [0,0,0],//（可选，默认为[0, 0, 0]）使模型相对于对应轴进行旋转，以角度为单位。
      "translation": [0, 0, 0] //可选，默认为[0, 0, 0]）使模型相对于对应轴进行平移，每个轴上的平移都钳制在-80到80之间。
    },
    "thirdperson_lefthand": {},
    "firstperson_righthand": {},
    "firstperson_lefthand": {},
    "gui": {},
    "head": {},
    "ground": {},
    "fixed": {}
  },
  "ambientocclusion": true,//（可选，若无继承属性默认为true）是否使用环境光遮蔽。
  "gui_light": "side/front",//（可选，若无继承属性默认为side）如果设置为side，那么在GUI内会按照方块类似的光照方式进行渲染；如果设置为front，则在GUI内会使用扁平物品的光照进行渲染。
  "textures": {//（可选）定义模型的使用的纹理变量列表。纹理变量可以从父模型继承。此列表内定义的纹理变量会覆盖父模型的纹理变量。纹理变量particle是特殊纹理变量，除了可以作为正常的纹理变量外，还控制了方块的粒子和下界传送门内的纹理。
    "0": "railcraftneo:block/depslate_zinc_ore",//定义一个纹理变量。可以是一个命名空间ID，也可以是另一个纹理变量（以#开头）。
    "particle": "railcraftneo:block/depslate_zinc_ore"
  },
  "elements": {
    "1": {//(可选，默认继承父模型)列出模型的所有元素。这些元素只能为长方体形式。
      "shade": true, //是否渲染阴影
      "rotation": {//设置元素的旋转
        "angle": "0", //旋转角度。只能为-45、-22.5、0、22.5和45。
        "axis": "x", //旋转轴，可以为x、y和z。
        "origin": [0, 0, 0], //设置旋转中心。
        "rescale": false //可选，默认为false）是否缩放为整个长方体大小。
      },
      "from": [0, 0, 0],
      "to": [16, 16, 16],
      "faces": {//模型元素的所有面。若有一个面没有定义，则不渲染此面。
        "down": {
          "uv": [0, 0, 16, 16],//（可选，默认根据当前面自动设置区域）按照[x1, y1, x2, y2]的格式，指定所使用的纹理图片的区域。若此值低于0或高于16，则纹理显示将会不正常。若x1和x2坐标对调，纹理也将会被翻转（例：从原本的0, 0, 16, 16变为16, 0, 0, 16）
          "texture": "#0", //指定所使用的纹理变量（以#开头）。
          "tintindex": -1, //(可选，默认为-1)使用硬编码的着色索引对纹理重新着色。只有下方列出的方块可以着色，其他方块设置此项无效。如果设置为-1，则不进行重新着色。
          "rotation": 0, //(可选，默认为0)根据特定的角度旋转纹理。可以为0、90、180或270。旋转不会影响到uv所指定的纹理部分，而是此面的旋转角度。
          "cullface": "down"//（可选，默认为当前面的同侧）指定当这个方块与指定方向上与毗邻方块接连时，这个面是否无需渲染。同时该面的亮度计算也等同于方块的指定面。方向可以为down、up、north、south、west和east。
        },
        "up": {},
        "north": {},
        "south": {},
        "west": {},
        "east": {}
      }
    }
  },
  "overrides": {
    "": {
      "predicate": "",
      "model": ""
    }
  }
}